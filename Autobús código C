#include <stdio.h>
#include <stdlib.h>
 int calcularPasajeros(int cambios[][2], int t)
{
    int resultado;
    int pasajerosSuben=0, pasajerosBajan=0;
 int i,j;
 for (i=0; i<5; i++)
 {
    for (j=0; j<1; j++)
 {
        pasajerosSuben= pasajerosSuben+cambios[i][j];

}
     for (j=1; j<2; j++)
        pasajerosBajan= pasajerosBajan+cambios[i][j];
}
 resultado= pasajerosSuben-pasajerosBajan;
 return resultado;
}



int main()
{
     int numPasajerosFinal;
     int numPasajeros [][2]={{},{},{},{},{}};
     int tamano=sizeof(numPasajeros)/sizeof(int);
     numPasajerosFinal= calcularPasajeros(numPasajeros,tamano);
  printf("El total de pasajeros que quedan es: %d\n", numPasajerosFinal);
