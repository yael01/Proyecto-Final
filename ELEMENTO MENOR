#include <stdio.h>
int hallarMenorElemento(int arreglo [], int t)
{
int men, x;
men = arreglo[0];

for (x=0; x<t; x++)
{
if (arreglo[x]<men)
{
men=arreglo[x];
}
}
printf("Este es el menor %d\n", men);
return 0;
}

