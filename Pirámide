#include <stdio.h>

int main()
{
    int a, b, c;
    printf("Ingresa un n√∫mero ");
    scanf("%d",&a);

    for(b=1;b<=a;b++)
    {
        for(c=1;c<=b;c++)
        {
            printf("%d",b);
        }
        printf("\n");
    }
    
    return 0;
}


